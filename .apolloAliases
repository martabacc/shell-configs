# aliasing sshs
source ~/.aliases/.sshAliases/.internals

alias cmt='sudo chmod -R 777 $(pwd)'
alias adbr='adb reverse tcp:8081 tcp:8081 && adb reverse tcp:8080 tcp:8080'
alias kelinci='ssh -p 2222 root@128.199.135.43'
alias sonarscanner='/Users/it190118/Downloads/sonarscanner/bin/sonar-scanner'
alias sonar-scanner='sonarscanner'
alias arvt='ssh -p 2333 root@206.189.44.253'
alias cdbl='cd /Users/it190118/sideprojects/ronayumik.github.io'
alias cdsp='cd /Users/it190118/sideprojects'
alias stash='stash --include-untracked'
alias c8='su 18055279'
alias cdas='cd /Users/it190118/apollo/apollo-service'
alias pubnpmap='npm publish --registry http://nexus.corp.bankbtpn.co.id:8081/repository/npm-apollo/'
alias pwcp='pwd | pbcopy'
